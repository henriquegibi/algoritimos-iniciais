Algoritmo "processamento de dados"
// Disciplina  : Estrutura sequencial
// Professor   : Nélio Alves
// Aluno       : Henrique Gibi de Pádua
// Data atual  : 30/08/2020
Var
   b1, b2, h, area : real
   
   a : real
   b : inteiro

Inicio
   b1 <- 6.0
   b2 <- 8.0
   h <- 5.0
   
   area <- (b1 + b2) / 2.0 * h

   escreval(area)


   a <- 5.3
   b <- Int(a)
   
   escreval(b)

Fimalgoritmo