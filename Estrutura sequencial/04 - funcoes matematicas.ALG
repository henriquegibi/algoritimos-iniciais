Var
   x, y, z, a, b, c, d : real

Inicio
   x <- 2.0
   y <- 3.0
   z <- -7.0
   
   a <- Exp(x, y)
   escreval(a)
   
   b <- RaizQ(y)
   escreval(b)
   
   c <- Pi
   escreval(y)
   
   d <- Abs(z)
   escreval(d)

Fimalgoritmo